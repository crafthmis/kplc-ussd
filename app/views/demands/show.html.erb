<div class="page-header">
  <%= link_to demands_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_demand_path(@demand), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show demand</h1>
</div>

<dl class="dl-horizontal">
  <dt>Request:</dt>
  <dd><%= @demand.request_id %></dd>

  <dt>Request timestamp:</dt>
  <dd><%= @demand.request_timestamp %></dd>

  <dt>Channel:</dt>
  <dd><%= @demand.channel %></dd>

  <dt>Operation:</dt>
  <dd><%= @demand.operation %></dd>

  <dt>Offer code:</dt>
  <dd><%= @demand.offer_code %></dd>

  <dt>Customer:</dt>
  <dd><%= @demand.customer %></dd>

  <dt>Link:</dt>
  <dd><%= @demand.link_id %></dd>

  <dt>Content:</dt>
  <dd><%= @demand.content %></dd>

</dl>
