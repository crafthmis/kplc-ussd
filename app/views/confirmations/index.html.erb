





<% content_for :index do %>
  <div class="table-responsive">
    <table class="table table-striped table-bordered table-hover">
          <thead>
      <tr>
            <th>Seqnum</th>
            <th>Create date</th>
            <th>Billrefnumber</th>
            <th>Transamount</th>
            <th>Transid</th>
            <th>Transtype</th>
            <th>Kptransid</th>
            <th>Mobilenumber</th>
            <th>Transtime</th>
            <th>Invoicenumber</th>
            <th>Messages</th>
            <th>Status</th>
            <th>Lastamount</th>
            <th>Messageid</th>
            <th>Method</th>
            <th>Userid</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

   
    <tbody>
      <%= content_tag_for(:tr, @confirmations) do |confirmation| %>
            <td><%= confirmation.seqnum %></td>
            <td><%= confirmation.create_date %></td>
            <td><%= confirmation.billrefnumber %></td>
            <td><%= confirmation.transamount %></td>
            <td><%= confirmation.transid %></td>
            <td><%= confirmation.transtype %></td>
            <td><%= confirmation.kptransid %></td>
            <td><%= confirmation.mobilenumber %></td>
            <td><%= confirmation.transtime %></td>
            <td><%= confirmation.invoicenumber %></td>
            <td><%= confirmation.messages %></td>
            <td><%= confirmation.status %></td>
            <td><%= confirmation.lastamount %></td>
            <td><%= confirmation.messageid %></td>
            <td><%= confirmation.method %></td>
            <td><%= confirmation.userid %></td>
            <% if confirmation.seqnum != nil %>
                <td><%= link_to 'Show', confirmation_path(confirmation.seqnum)  %></td>
                <td><%= link_to 'Edit', edit_confirmation_path(confirmation.seqnum) %></td>
                <td><%= link_to 'Destroy', confirmation_path(confirmation.seqnum), method: :delete, data: { confirm: 'Are you sure?' }  %></td>
            <%end%>

      <% end %>
    </tbody>

    </table>
  </div>
<%end%>

