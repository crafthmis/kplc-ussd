<%= form_for(@confirmation, html: { class: "form-horizontal", role: "form" }) do |f| %>
  <% if @confirmation.errors.any? %>
    <div class="alert alert-danger alert-dismissable" role="alert">
      <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
      <h4><%= pluralize(@confirmation.errors.count, "error") %> prohibited this confirmation from being saved:</h4>

      <ul>
      <% @confirmation.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :seqnum, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.number_field :seqnum, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :create_date, class: "col-sm-2 control-label" %>
