<div class="page-header">
  <%= link_to complaints_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_complaint_path(@complaint), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show complaint</h1>
</div>

<dl class="dl-horizontal">
  <dt>Customer:</dt>
  <dd><%= @complaint.customer %></dd>

  <dt>Info:</dt>
  <dd><%= @complaint.info %></dd>

  <dt>Complaintable:</dt>
  <dd><%= @complaint.complaintable %></dd>

</dl>
