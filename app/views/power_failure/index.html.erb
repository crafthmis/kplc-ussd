<% @minititle = "Analytics Dashboard" %>


<div class="row">
          <div class="col-lg-6 col-md-6 col-sm-6">
            <div class="card card-stats">
              <div class="card-body ">
                <div class="row">
                  <div class="col-5 col-md-4">
                    <div class="icon-big text-center icon-warning">
                      <i class="nc-icon nc-sun-fog-29 text-warning"></i>
                    </div>
                  </div>
                  <div class="col-7 col-md-8">
                    <div class="numbers">
                      <p class="card-category">Prepaid</p>
                      <p class="card-title"><%#= @prepaid.size %><p>
                    </div>
                  </div>
                </div>
              </div>
                        </div>
          </div>
          <div class="col-lg-6 col-md-6 col-sm-6">
            <div class="card card-stats">
              <div class="card-body ">
                <div class="row">
                  <div class="col-5 col-md-4">
                    <div class="icon-big text-center icon-warning">
                      <i class="nc-icon nc-bulb-63  text-success"></i>
                    </div>
                  </div>
                  <div class="col-7 col-md-8">
                    <div class="numbers">
                      <p class="card-category">Postpaid</p>
                      <p class="card-title"><%#= @postpaid.size %><p>
                    </div>
                  </div>
                </div>
              </div>
             
            </div>
          </div>
         
             
        </div>
        <div class="row">
          <div class="col-md-12">
            <div class="card ">
              <div class="card-header ">
                <h5 class="card-title">Unique Requests</h5>
              </div>

              <%#= content_tag :div, "",id: "request_labels",data: {requests: request_by_hour(@request_chart,@prepaid,@postpaid,@power_failure,@jua_for_sure,@period,@manage_account)} %>
              <div class="card-body ">
                <div id="request_charts"></div>
              </div>
           
            </div>
          </div>
        </div>


        <div class="row">
          <div class="col-md-12">
            <div class="card ">
              <div class="card-header ">
                <h5 class="card-title">New Users Vs Unique Requests</h5>
              </div>

              <%#= content_tag :div, "",id: "daily_labels",data: {daily: request_chart_data(@request_chart,@customer_chart)  } %>
              
              <div class="card-body ">
                <div id="daily_requests"></div>
              </div>
         
            </div>
          </div>
       
          

        </div>




