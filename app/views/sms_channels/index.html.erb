<div class="page-header">
  <%= link_to new_sms_channel_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Sms channel
  <% end %>
  <h1>Listing sms_channels</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Type</th>
            <th>Shortcode</th>
            <th>Alphanumeric</th>
            <th>Keyword</th>
            <th>Organization</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @sms_channels) do |sms_channel| %>
            <td><%= sms_channel.type %></td>
            <td><%= sms_channel.shortcode %></td>
            <td><%= sms_channel.alphanumeric %></td>
            <td><%= sms_channel.keyword %></td>
            <td><%= sms_channel.organization %></td>
            <td><%= link_to 'Show', sms_channel %></td>
        <td><%= link_to 'Edit', edit_sms_channel_path(sms_channel) %></td>
        <td><%= link_to 'Destroy', sms_channel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
